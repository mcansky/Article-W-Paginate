<table>
  <tr>
    <th>Titre</th>
  </tr>
  <% for post in posts %>
    <tr>
      <td><%= link_to post.title, post %></td>
      <td><%= link_to "Editer", edit_post_path(post) %></td>
      <td><%= link_to_remote "Supprimer", :update => "posts", :url => {:action => "delete", :id => post.id, :page => params[:page]}, :confirm => 'Etes vous sÃ»r ?' %></td>
    </tr>
  <% end %>
</table>
<%= will_paginate posts, :params => {:action => "index"} %>